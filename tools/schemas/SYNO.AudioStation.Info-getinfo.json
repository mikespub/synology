  "required": [
    "ame_status",
    "browse_personal_library",
    "dsd_decode_capability",
    "enable_equalizer",
    "enable_personal_library",
    "enable_user_home",
    "has_music_share",
    "is_manager",
    "playing_queue_max",
    "privilege",
