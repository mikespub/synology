{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "ame_status": {
      "type": "object",
      "properties": {
        "ame_major_version": {
          "type": "integer"
        },
        "has_aac": {
          "type": "boolean"
        },
        "has_license": {
          "type": "boolean"
        },
        "is_aac_activated": {
          "type": "boolean"
        },
        "is_ame_broken": {
          "type": "boolean"
        },
        "is_ame_install": {
          "type": "boolean"
        },
        "need_aac_transcoding": {
          "type": "boolean"
        }
      },
      "required": [
        "ame_major_version",
        "has_aac",
        "has_license",
        "is_aac_activated",
        "is_ame_broken",
        "is_ame_install",
        "need_aac_transcoding"
      ]
    },
    "browse_personal_library": {
      "type": "string"
    },
    "dsd_decode_capability": {
      "type": "boolean"
    },
    "enable_equalizer": {
      "type": "boolean"
    },
    "enable_personal_library": {
      "type": "boolean"
    },
    "enable_user_home": {
      "type": "boolean"
    },
    "has_music_share": {
      "type": "boolean"
    },
    "is_manager": {
      "type": "boolean"
    },
    "playing_queue_max": {
      "type": "integer"
    },
    "privilege": {
      "type": "object",
      "properties": {
        "playlist_edit": {
          "type": "boolean"
        },
        "remote_player": {
          "type": "boolean"
        },
        "sharing": {
          "type": "boolean"
        },
        "tag_edit": {
          "type": "boolean"
        },
        "upnp_browse": {
          "type": "boolean"
        }
      },
      "required": [
        "playlist_edit",
        "remote_player",
        "sharing",
        "tag_edit",
        "upnp_browse"
      ]
    },
    "remote_controller": {
      "type": "boolean"
    },
    "same_subnet": {
      "type": "boolean"
    },
    "serial_number": {
      "type": "string"
    },
    "settings": {
      "type": "object",
      "properties": {
        "audio_show_virtual_library": {
          "type": "boolean"
        },
        "disable_upnp": {
          "type": "boolean"
        },
        "enable_download": {
          "type": "boolean"
        },
        "prefer_using_html5": {
          "type": "boolean"
        },
        "transcode_to_mp3": {
          "type": "boolean"
        }
