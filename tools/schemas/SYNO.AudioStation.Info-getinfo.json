    "sid": {
      "type": "string"
    },
    "support_bluetooth": {
      "type": "boolean"
    },
    "support_usb": {
      "type": "boolean"
    },
    "support_virtual_library": {
      "type": "boolean"
    },
    "transcode_capability": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "version": {
      "type": "integer"
    },
    "version_string": {
      "type": "string"
    }
