{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "arp_ignore": {
      "type": "boolean"
    },
    "dns_manual": {
      "type": "boolean"
    },
    "dns_primary": {
      "type": "string"
    },
    "dns_secondary": {
      "type": "string"
    },
    "enable_ip_conflict_detect": {
      "type": "boolean"
    },
    "enable_windomain": {
      "type": "boolean"
    },
    "gateway": {
      "type": "string"
    },
    "gateway_info": {
      "type": "object",
      "properties": {
        "ifname": {
          "type": "string"
        },
        "ip": {
          "type": "string"
        },
        "mask": {
          "type": "string"
        },
        "status": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "use_dhcp": {
          "type": "boolean"
