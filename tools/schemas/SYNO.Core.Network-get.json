      "type": "boolean"
    },
    "multi_gateway": {
      "type": "boolean"
    },
    "server_name": {
      "type": "string"
    },
    "v6gateway": {
      "type": "string"
    }
  },
  "required": [
    "arp_ignore",
    "dns_manual",
    "dns_primary",
    "dns_secondary",
    "enable_ip_conflict_detect",
    "enable_windomain",
    "gateway",
