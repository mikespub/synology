{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "enable_reset_passwd_by_email": {
      "type": "boolean"
    },
    "password_must_change": {
      "type": "boolean"
    },
    "strong_password": {
      "type": "object",
      "properties": {
        "exclude_username": {
          "type": "boolean"
        },
        "history_num": {
          "type": "integer"
        },
        "included_numeric_char": {
          "type": "boolean"
        },
        "included_special_char": {
          "type": "boolean"
        },
        "min_length": {
          "type": "integer"
        },
        "min_length_enable": {
          "type": "boolean"
        },
        "mixed_case": {
          "type": "boolean"
        }
      },
      "required": [
        "exclude_username",
        "history_num",
        "included_numeric_char",
        "included_special_char",
        "min_length",
        "min_length_enable",
        "mixed_case"
      ]
    }
  },
  "required": [
    "enable_reset_passwd_by_email",
    "password_must_change",
    "strong_password"
  ]
}
